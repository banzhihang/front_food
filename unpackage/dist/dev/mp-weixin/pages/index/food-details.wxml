<view class="content"><view class="header"><view class="food-picture uni-thumb"><image src="{{food.first_image}}" mode="aspectFill"></image></view><view><text class="food-name">{{food.foodName}}</text><view class="food-time"><image class="food-time-img" src="../../static/img/time.png"></image><text>{{food.add_time}}</text></view><view class="food-address"><image class="food-address-img" src="../../static/img/address.png"></image><text>{{food.address}}</text></view><view class="food-want-eated"><button class="food-want" type="primary" size="mini">想吃</button><button class="food-eated" type="primary" size="mini">吃过</button></view></view></view><view class="score"><uni-card vue-id="02cd23fd-1" is-full="false" title="吃小猪评分" thumbnail="https://vkceyugu.cdn.bspapp.com/VKCEYUGU-dc-site/460d46d0-4fcc-11eb-8ff1-d5dcf8779628.png" extra="{{food.vote_number}}" isShadow="{{true}}" bind:__l="__l" vue-slots="{{['default']}}"><view class="score-text"><text>{{food.score}}</text><uni-rate vue-id="{{('02cd23fd-2')+','+('02cd23fd-1')}}" value="{{$root.m0}}" size="0.5" allowHalf="{{true}}" readonly="{{true}}" bind:__l="__l"></uni-rate></view></uni-card></view><view class="desc"><uni-card vue-id="02cd23fd-3" title="简介" thumbnail="https://vkceyugu.cdn.bspapp.com/VKCEYUGU-dc-site/460d46d0-4fcc-11eb-8ff1-d5dcf8779628.png" isShadow="{{true}}" bind:__l="__l" vue-slots="{{['default']}}"><text>{{food.desc}}</text></uni-card></view><view class="food-photo"></view></view>