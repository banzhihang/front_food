/* 文章场景相关 */
.uni-flex, .uni-flex-row, .uni-list-box, .uni-media-box, .uni-footer, .uni-flex-column, .uni-content {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.uni-flex-row, .uni-list-box, .uni-media-box, .uni-footer {
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  box-sizing: border-box;
}
.uni-flex-column, .uni-content {
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}
.uni-color-gary {
  color: #3b4144;
}
/* 标题 */
.uni-title {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  margin-bottom: 8px;
  font-size: 16;
  font-weight: bold;
  color: #3b4144;
}
.uni-title-sub {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 14px;
  font-weight: 500;
  color: #3b4144;
}
/* 描述 额外文本 */
.uni-note {
  margin-top: 10px;
  color: #999;
  font-size: 12px;
}
/* 列表内容 */
.uni-list-box {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  margin-top: 10px;
}
/* 略缩图 */
.uni-thumb {
  -webkit-flex-shrink: 0;
          flex-shrink: 0;
  margin-right: 10px;
  width: 125px;
  height: 75px;
  border-radius: 6px;
  overflow: hidden;
  border: 1px #f5f5f5 solid;
}
.uni-thumb image {
  width: 100%;
  height: 100%;
}
.uni-media-box {
  border-radius: 6px;
  overflow: hidden;
}
.uni-media-box .uni-thumb {
  margin: 0;
  margin-left: 4px;
  -webkit-flex-shrink: 1;
          flex-shrink: 1;
  width: 33%;
  border-radius: 0;
}
.uni-media-box .uni-thumb:first-child {
  margin: 0;
}
/* 内容 */
.uni-content {
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
/* 列表footer */
.uni-footer {
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  margin-top: 12px;
}
.uni-footer-text {
  font-size: 12px;
  color: #999;
  margin-left: 5px;
}
/* 标签 */
.uni-tag {
  -webkit-flex-shrink: 0;
          flex-shrink: 0;
  padding: 0 5px;
  border: 1px #c8c7cc solid;
  margin-right: 5px;
  border-radius: 3px;
  background: #f8f8f8;
  color: #333;
  font-size: 12px;
}
/* 链接 */
.uni-link {
  margin-left: 10px;
  color: #333;
  text-decoration: underline;
}
.food-list {
  padding: 0 5rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.food-list .food-items {
  background: #fff;
  width: 355rpx;
  box-sizing: border-box;
}
.middle-desc {
  font-size: 18rpx;
  line-height: 50rpx;
  padding-bottom: 15rpx;
  padding-top: 10rpx;
}
.food-picture {
  height: 70px;
  width: 60px;
}
.want-eat {
  height: 30px;
  width: 60px;
  font-size: 5px;
}
.want-eat .want-eat-btn {
  position: absolute;
  right: 0;
  top: 20;
  background-color: #F7CC5B;
}
.middle-rate {
  margin-top: 5px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.middle-rate .score-text {
  margin-top: -6rpx;
  margin-left: 7rpx;
  font-size: 4rpx;
  color: #F7CC5B;
}

